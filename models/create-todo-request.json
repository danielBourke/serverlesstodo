{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "Todo",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "todoId": {
            "type": "string"
        },
        "createdAt":{
            "type": "string"
        },
        "dueDate":{
            "type":"string"
        },
        "done":{
            "type":"boolean"
        },
        "attachmentUrl":{
            "type":"string"
        }
    }, "required":[
        "name",
        "createdAt",
        "dueDate",
        "done"
    ],
    "additionalProperties": false
}